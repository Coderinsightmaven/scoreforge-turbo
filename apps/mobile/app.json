{
  "expo": {
    "name": "mobile",
    "slug": "mobile",
    "version": "1.0.0",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true,
      "reactCompiler": true
    },
    "plugins": [],
    "runtimeVersion": {
      "policy": "appVersion"
    },
